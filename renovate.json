{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "regexManagers": [
    {
      "fileMatch": ["^rust-toolchain$"],
      "matchStrings": [
        "(?<currentValue>[0-9.]+)"
      ],
      "datasourceTemplate": "github-releases",
      "lookupNameTemplate": "rust-lang/rust",
      "depNameTemplate": "rust"
    }
  ]
}
